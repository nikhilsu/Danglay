<%= form_for(join_cabpool[:cabpool], url: '/cabpools/join', method: 'post', :html => join_cabpool[:popup_action_js]) do |f| %>
    <%= f.hidden_field(:id) %>
    <% button = button_info(join_cabpool[:cabpool]) %>
    <%= f.button button[:name], class: join_cabpool[:join_button_style], disabled: button[:disabled]%>
<% end %>